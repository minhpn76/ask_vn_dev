{
  "name": "react-boilerplate",
  "version": "4.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "yarn analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node -r esm ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "yarn npmcheckversion",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production --env.ENVIRONMENT=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "build:dev": "npm run build",
    "start": "cross-env NODE_ENV=development --env.ENVIRONMENT=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "yarn test && yarn build && yarn start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "yarn add chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "yarn analyze:clean && yarn test:clean && yarn build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:eslint": "eslint . --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint . --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "yarn lint:eslint -- . ",
    "lint:ts": "yarn lint:tslint",
    "lint:tslint": "tslint --config ./tslint.json --project ./tsconfig.json --fix && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "_serve:mocks": "cd ./mock-server && node mockserver.js"
  },
  "eslintIgnore": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint:eslint:fix",
      "git add --force"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add --force"
    ],
    "*.ts": "yarn lint:tslint",
    "*.tsx": "yarn lint:tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:tslint"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
